<struts>
	<package name="datashare.sign.mail" extends="default" namespace="/datashare/email/sign">
		 <action name="*" class="signMailAction" method="{1}">
		 	 <result name="index">/pages/datashare/email/sign/index.jsp</result>
		     <result name="input">/pages/datashare/email/sign/edit.jsp</result>
		     <result name="login">/login.jsp</result>
		     <result name="success" type="redirect">index.do</result>
		 </action>
		
	</package>
</struts>
