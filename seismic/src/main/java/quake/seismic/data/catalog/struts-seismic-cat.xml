<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="quake.seismic.data.catalog" extends="default" namespace="/quake/seismic/data/catalog">
		 <action name="*" class="catAction" method="{1}">
		 	 <!-- 地震目录查询(按矩形区域)  -->
		     <result name="list">/pages/quake/seismic/data/catalog/index.jsp</result>
		     <result name="index">/pages/quake/seismic/data/catalog/index.jsp</result>
		     <!-- 地震目录查询(按圆形区域)  -->
		     <result name="listRound">/pages/quake/seismic/data/catalog/indexRound.jsp</result>
		     <result name="indexRound">/pages/quake/seismic/data/catalog/indexRound.jsp</result>
		     <!--  震相查询  -->
		     <result name="indexPhase">/pages/quake/seismic/data/phase/index.jsp</result>
		     <result name="listPhase">/pages/quake/seismic/data/phase/index.jsp</result>
		     <result name="quake">/pages/quake/seismic/data/catalog/eq.jsp</result>
		     <!-- Seed查询 -->
		     <result name="indexSeed">/pages/quake/seismic/data/seed/index.jsp</result>
		     <result name="listSeed">/pages/quake/seismic/data/seed/index.jsp</result>
		 </action>
	</package>
</struts>
