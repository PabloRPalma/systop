				<isNull prepend="AND" property="endM">
				<![CDATA[(M>=#startM#)]]>
				</isNull>
			</isNotNull>
			<isNotNull property="endM">
				<isNull prepend="AND" property="startM">
				<![CDATA[(M<=#endM#)]]>
				</isNull>
			</isNotNull>
			<!--只显示大于-10的震级-->
			<isNull property="startM">
				<isNull prepend="AND" property="endM">
					<![CDATA[(M>-10)]]>
				</isNull>
			</isNull>
		</dynamic>
		
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortProperty">
