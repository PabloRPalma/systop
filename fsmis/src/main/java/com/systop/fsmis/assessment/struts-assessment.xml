<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fsmis.assessment" extends="json"  namespace="/assessment">
		<action name="*" class="assessmentAction" method="{1}">
			<result name="index">/pages/assessment/index.jsp</result> 
			<result name="input">/pages/assessment/edit.jsp</result> 	
			<result name="audit">/pages/assessment/audit.jsp</result> 
			<result name="start">/pages/assessment/start.jsp</result> 
			<result name="result">/pages/assessment/result.jsp</result> 						
			<result name="print">/pages/assessment/printpage.jsp</result> 
			<result name="listCheckRst">/pages/assessment/checkRstView.jsp</result>
			<result name="process">/pages/assessment/flow/process.jsp</result>			
			<result name="view">/pages/assessment/view.jsp</result> 														
			<result name="success" type="redirect">index.do</result>	
			<result type="json" name="titles">
		        <param name="ignoreHierarchy">false</param>
		        <param name="root">jsonTitles</param>
		    </result>	
			<result type="json" name="members">
		        <param name="ignoreHierarchy">false</param>
		        <param name="root">jsonMembers</param>
		    </result>
			<result type="json" name="leaders">
		        <param name="ignoreHierarchy">false</param>
		        <param name="root">jsonLeaders</param>
		    </result>		    		    		
		</action>
	</package>
	<package name="fsmis.assessment.member" extends="json" namespace="/assessment/member">
		<action name="*" class="membersSelectorAction" method="{1}">
			 <result type="json" name="json">
		        <param name="ignoreHierarchy">false</param>
		        <param name="root">page</param>
		    </result>	    
		</action>		
	</package>
	<!-- 风险评估附件请求处理 -->
	 <package name="fsmis.assessment.attaches" extends="json"  namespace="/assessment/attaches">    
	   <action name="*" class="asseAttachAction" method="{1}">
	     <result name="index">/pages/assessment/attaches.jsp</result> 
	     <result name="input">/pages/assessment/attaches.jsp</result> 
	     <result name="success" type="redirect">index.do?assessmentId=${assessmentId}</result>    
	     <result name="view">/pages/assessment/attachesView.jsp</result> 	               
	   </action>
	 </package>  	
</struts>

