<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fsmis.sendType" extends="default"  namespace="/sendType">
		<action name="*" class="sendTypeAction" method="{1}">
			<result name="index">/pages/fsecurity/singleEvent/sendType/list.jsp</result> 
			<result name="verify">/pages/fsecurity/singleEvent/sendType/verify.jsp</result> 
			<result name="input">/pages/fsecurity/singleEvent/sendType/edit.jsp</result> 
			<result name="look">/pages/fsecurity/singleEvent/sendType/look.jsp</result>
			<result name="choose">/pages/fsecurity/singleEvent/sendType/choosesendtype.jsp</result>
			<result name="send">/pages/fsecurity/singleEvent/task/task.jsp</result>
			<result name="success" type="redirect">index.do</result> 
		</action>
	</package>
</struts>

