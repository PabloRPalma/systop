<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fsmis.fscase" extends="json"  namespace="/fscase">
		<action name="*" class="fsCaseAction" method="{1}">
			<result name="index">/pages/fsCase/list.jsp</result> 
			<result name="input">/pages/fsCase/edit.jsp</result> 
			<result name="view">/pages/fsCase/view.jsp</result>
			<result name="success" type="redirect">index.do</result> 
			<result type="json" name="jsonRst">
		   		<param name="root">typeRst</param>
			</result>
		</action>
	</package>
</struts>

