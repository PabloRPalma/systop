<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fsmis.genericCase" extends="default"  namespace="/genericCase">
		<action name="*" class="genericCaseAction" method="{1}">
			<result name="index">/pages/genericCase/list.jsp</result> 
			<result name="input">/pages/genericCase/edit.jsp</result> 
			<result name="view">/pages/genericCase/view.jsp</result>
			<result name="look">/pages/genericCase/look.jsp</result>
			<result name="success" type="redirect">index.do</result> 
		</action>
	</package>
</struts>

