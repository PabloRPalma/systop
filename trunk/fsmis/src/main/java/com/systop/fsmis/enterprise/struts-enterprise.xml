<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fsmis.enterprise" extends="default"  namespace="/enterprise">
		<action name="*" class="enterpriseAction" method="{1}">
			<result name="index">/pages/enterprise/index.jsp</result> 
			<result name="input">/pages/enterprise/edit.jsp</result> 
			<result name="success" type="redirect">index.do</result>
			<result name="look">/pages/enterprise/look.jsp</result>
			<result name="error">/pages/enterprise/error.jsp</result>
			<result name="licenseexpiredcount">/pages/enterprise/licenseExpiredCount.jsp</result>			
		</action>
	</package>
</struts>

