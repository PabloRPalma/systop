<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fsmis.office.notice" extends="json"  namespace="/office/notice">
		<action name="*" class="noticeAction" method="{1}">
			<result name="index">/pages/office/notice/list.jsp</result> 
			<result name="input">/pages/office/notice/edit.jsp</result> 
			<result name="view">/pages/office/notice/view.jsp</result>
			<result name="success" type="redirect">index.do</result> 
		</action>
	</package>
</struts>

